{
  "buildCommand": "npm install -g pnpm && pnpm install && pnpm run build",
  "outputDirectory": "build",
  "framework": null,
  "headers": [
    {
      "source": "/(.*).wasm",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/wasm"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/((?!.*\\.).*)",
      "destination": "/index.html"
    }
  ]
}
