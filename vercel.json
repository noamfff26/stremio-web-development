{
  "buildCommand": "pnpm install && pnpm run build",
  "installCommand": "npm install -g pnpm",
  "outputDirectory": "build",
  "headers": [
    {
      "source": "/(.*)\\.wasm",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/wasm"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
